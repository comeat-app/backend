    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.0",
    "fastify-type-provider-zod": "^6.0.0",
    "prisma": "^6.15.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.5.2",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
